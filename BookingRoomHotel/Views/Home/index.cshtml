    <!-- Check availablity Once -->
    <section class="ftco-booking">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <form asp-action="Index" asp-controller="Home" onsubmit="saveSearchInfo()" class="booking-form" id="formSearch">
                        <div class="row justify-content-between">
                            <div class="col-md-2 d-flex">
                                <div class="form-group p-4 align-self-stretch d-flex align-items-center">
                                    <div class="wrap">
                                        <label for="#">Check-in</label>
                                        <div class="select-wrap">
                                            <input asp-for="SearchRoomViewModel.CheckInDate" name="CheckInDate" type="text" class="form-control checkin_date">
                                            <div class="icon"><span class="bi bi-caret-down"></span></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2 d-flex">
                                <div class="form-group p-4 align-self-stretch d-flex align-items-center">
                                    <div class="wrap">
                                        <label for="#">Check-out</label>
                                        <div class="select-wrap">
                                            <input asp-for="SearchRoomViewModel.CheckOutDate" name="CheckOutDate" type="text" class="form-control checkout_date" placeholder="">
                                            <div class="icon"><span class="bi bi-caret-down"></span></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3 d-flex">
                                <div class="form-group p-4 align-self-stretch d-flex align-items-center">
                                    <div class="wrap">
                                        <label for="#">Room Types</label>
                                        <div class="form-field">
                                            <div class="select-wrap">
                                                <div class="icon"><span class="bi bi-caret-down"></span></div>
                                                <select asp-for="SearchRoomViewModel.RoomType" name="RoomType" id="" class="form-control">
                                                    <span class="bi bi-caret-down"></span>
                                                    <option value="" style="display:none;"></option>
                                                    <option value="1">Suite Room</option>
                                                    <option value="2">Classic Room</option>
                                                    <option value="3">Family Room</option>
                                                    <option value="4">Deluxe Room</option>
                                                    <option value="5">Superior Room</option>
                                                    <option value="6">Luxury Room</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2 d-flex">
                                <div class="form-group p-4 align-self-stretch d-flex align-items-center">
                                    <div class="wrap">
                                        <label for="#">Area</label>
                                        <div class="form-field">
                                            <div class="select-wrap">
                                                <span> > </span><output id="outArea">30</output><span> m<sup>2</sup></span>
                                                <input asp-for="SearchRoomViewModel.AreaFrom" name="Area" type="range" value="30" min="30" step="5" max="100" oninput="document.getElementById('outArea').value = this.value">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2 d-flex">
                                <div class="form-group p-4 align-self-stretch d-flex align-items-center">
                                    <div class="wrap">
                                        <label for="#">Price</label>
                                        <div class="form-field">
                                            <div class="select-wrap">
                                                <span> > </span><output id="outPrice">100</output><span> $</span>
                                                <input asp-for="SearchRoomViewModel.PriceFrom" name="Price" type="range" value="100" min="100" step="100" max="5000" oninput="document.getElementById('outPrice').value = this.value">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md d-flex">
                                <div class="form-group d-flex align-self-stretch">
                                    <input type="submit" class="btn btn-primary py-3 px-4 align-self-stretch">
                                </div>
                            </div>
                        </div>
                        <span id="SearchError" class="text-danger"></span>
                    </form>
                </div>
                @Html.ValidationSummary(false, "", new { @class = "text-danger" })
            </div>
        </div>
    </section>
    <div class="container">
