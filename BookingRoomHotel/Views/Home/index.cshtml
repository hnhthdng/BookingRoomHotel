    <div id="add_event1" class="modal fade" role="dialog">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Add New Booking</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"> <span aria-hidden="true">&times;</span> </button>
                </div>
                <div class="modal-body">
                    <form onsubmit="return BookingRoom()" id="formBooking">
                        <input type="hidden" id="BookingRoomID" name="RoomID" />
                        <span id="RoomID" class="text-danger"></span>
                        <input type="hidden" id="IdCus" name="CustomerId" />
                        <span id="CustomerId" class="text-danger"></span>
                        <div class="row">
                            <div class="form-group col-lg-12">
                                <label>Check-in Date <span class="text-danger">*</span></label>
                                <div class="input-group">
                                    <input name="CheckInDate" class="form-control checkin_date" type="text">
                                    <span class="input-group-text">
                                        <i class="bi bi-calendar"></i>
                                    </span>
                                </div>
                                <span id="CheckInDate" class="text-danger"></span>
                            </div>

                            <div class="form-group col-lg-12">
                                <label>Check-out Date <span class="text-danger">*</span></label>
                                <div class="input-group">
                                    <input name="CheckOutDate" class="form-control checkout_date" type="text">
                                    <span class="input-group-text">
                                        <i class="bi bi-calendar"></i>
                                    </span>
                                </div>
                                <span id="CheckOutDate" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Service <span class="text-danger">*</span></label>
                            <input class="form-control" type="text">
                        </div>

                        <div class="d-flex form-group">
                            <label>Total Price: <span class="text-danger"></span></label>
                            <p><h5 class="price" id="BookingPrice"></h5></p>
                        </div>
                        <span id="CustomerId" class="text-danger"></span>
                        <div class="submit-section">
                            <button type="submit" class="btn btn-primary submit-btn">Checkout</button>
                        </div>

                        <span id="BookingError" class="text-danger"></span>
                    </form>
